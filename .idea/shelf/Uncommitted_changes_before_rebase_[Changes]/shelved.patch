+package org.example;
+
+public class Calculadora {
+
+    public int suma(int a, int b){
+        int res = a + b;
+        return res;
+    }
+
+    public int resta(int a, int b){
+        int res = a - b;
+        return res;
+    }
+    public int multiplicacion(int a, int b){
+        int res = a * b;
+        return res;
+    }
